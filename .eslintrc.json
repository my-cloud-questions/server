{
  "env": {
    "browser": true,
    "es2020": true,
    "jest/globals": true
  },
  "extends": ["standard"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["jest"],
  "rules": {
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "no-console": "error"
  },
  "globals": {"mockRequest":"writable", "mockResponse":"writable", "mockNext":"writable"}
}
